{
    "model": {
        "name": "models.arythmia_cnn_model.ArythmiaCNNModel",
        "hparams": {
            "class_num": 3,
            "learning_rate": 0.0003,
            "num_epochs": 4000,
            "normalize_inputs": false,
            "conv_layers": 5,
            "kernel_size": 3,
            "filters_num": 16,
            "use_batch_norm": true,
            "dense_layers": 1,
            "dense_units": 256,
            "use_dropout": true,
            "dropout_rate": 0.3         
        }
    },
    "experiment": "initial01",
    "dataset" : {
        "name" : "datasets.Arythmia.wave_dataset.ArythmiaWaveDataset",
        "params": {
            "db_name": "mitdb",
            "bypass_cache": false,
            "slice_window": 900,
            "rythm_filter": [{
                "name": "N",
                "rythm": "(N",
                "beats": [".", "N"],
                "use_augmentation": false
            }, {
                "name": "L",
                "rythm": null,
                "beats": ["L"],
                "use_augmentation": true
            }, {
                "name": "R",
                "rythm": null,
                "beats": ["R"],
                "use_augmentation": true
            }],
            "label_map": {
                "N": 0,
                "L": 1,
                "R": 2
            },
            "rythm_map": {},
            "split_ratio": [0.25, 0.25, 0.25, 0.25],
            "test_set_size": 0.0,
            "train_batch_size": 32,
            "eval_batch_size": 100,
            "include_augmented": false
        }
    },
    "max_to_keep": 5
}