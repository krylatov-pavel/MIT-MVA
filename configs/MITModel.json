{
    "model": {
        "name": "models.cnn_model.CNNModel",
        "hparams": {
            "learning_rate": 0.00003,
            "num_epochs": 10000,
            "class_num": 4            
        }
    },
    "experiment": "initial",
    "dataset" : {
        "name" : "datasets.MIT.dataset.Dataset",
        "params": {
            "db_name": "vfdb",
            "bypass_cache": false,
            "signal_type_filter": ["(ASYS", "(N", "(VF", "(VT"],
            "signal_type_map": {
                "(NSR" : "(N",
                "(VFIB": "(VF"
            },
            "split_ratio": 0.2,
            "sample_len": 750,
            "train_batch_size": 32,
            "eval_batch_size": 100
        }
    },
    "max_to_keep": 5
}