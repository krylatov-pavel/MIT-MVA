{
    "model": {
        "name": "models.cnn_model.CNNModel",
        "hparams": {
            "learning_rate": 0.00002,
            "num_epochs": 20000,
            "normalize_inputs": false,
            "conv_layers": 3,
            "kernel_size": 3,
            "filters_num": 16,
            "use_batch_norm": true,
            "dense_layers": 1,
            "dense_units": 128,
            "use_dropout": true,
            "dropout_rate": 0.5,
            "class_num": 2  
        }
    },
    "dataset": {
        "name": "datasets.MIT_2d.wave_dataset.WaveDataset",
        "params": {
            "rythm_filter": ["(N", "(VF"],
            "rythm_map": {
                "(NSR" : "(N",
                "(VFIB": "(VF",
                "(VT": "(VF"
            },
            "label_map": {
                "(N": 0,
                "(VF": 1
            },
            "db_name": "vfdb",
            "slice_window": 750
        }
    },
    "experiment": "refactored_dataset"
}