{
    "model": {
        "name": "models.cnn_model.CNNModel",
        "hparams": {
            "learning_rate": 0.00002,
            "num_epochs": 20000,
            "normalize_inputs": false,
            "conv_layers": 3,
            "kernel_size": 3,
            "filters_num": 16,
            "use_batch_norm": true,
            "dense_layers": 1,
            "dense_units": 128,
            "use_dropout": true,
            "dropout_rate": 0.5,
            "class_num": 2  
        }
    },
    "dataset": {
        "params": {
            "signal_type_filter": ["(N", "(VF"],
            "signal_type_map": {
                "(NSR" : "(N",
                "(VFIB": "(VF",
                "(VT": "(VF"
            },
            "db_name": "vfdb",
            "sample_len": 750
        }
    },
    "experiment": "2_classes"
}