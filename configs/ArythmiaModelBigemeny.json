{
    "model": {
        "name": "models.arythmia_cnn_model.ArythmiaCNNModel",
        "experiment": "3_classes_normalize",
        "hparams": {
            "class_num": 3,
            "learning_rate": 0.046511,
            "num_epochs": 4000,
            "conv_layers": 6,
            "kernel_size": 3,
            "filters_num": 32,
            "use_batch_norm": true,
            "dense_layers": 1,
            "dense_units": 128,
            "use_dropout": true,
            "dropout_rate": 0.332
        }
    },
    "dataset" : {
        "name" : "datasets.Arythmia.wave_dataset.ArythmiaWaveDataset",
        "params": {
            "db_name": "mitdb",
            "experiment": "4-fold_RNB_equalize_0.25overlap",
            "bypass_cache": false,
            "slice_window": 1080,
            "slice_overlap": 0.25,
            "rythm_filter": [{
                "name": "N",
                "rythm": "(N",
                "beats": [".", "N"],
                "use_augmentation": false
            }, {
                "name": "R",
                "rythm": null,
                "beats": ["R"],
                "use_augmentation": true
            }, {
                "name": "B",
                "rythm": "(B",
                "beats": [],
                "use_augmentation": true
            }],
            "label_map": {
                "N": 0,
                "R": 1,
                "B": 2
            },
            "equalize_classes": true,
            "rescale": false,
            "normalize": true,      
            "rythm_map": {},
            "split_ratio": [0.25, 0.25, 0.25, 0.25],
            "test_set_size": 0.0,
            "train_batch_size": 32,
            "eval_batch_size": 100,
            "include_augmented": false
        }
    },
    "max_to_keep": 5
}